{
  "name": "slidev-theme-tum",
  "version": "0.2.2",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "slidev build example_2.md",
    "dev": "slidev example_2.md --open",
    "export": "slidev export example_2.md",
    "screenshot": "slidev export example_2.md --format png"
  },
  "dependencies": {
    "@slidev/theme-default": "^0.25.0",
    "@slidev/types": "^52.2.4",
    "sass-embedded": "^1.93.2"
  },
  "devDependencies": {
    "@slidev/cli": "^52.2.4",
    "sass-embedded": "^1.93.2"
  },
  "peerDependencies": {
    "sass-embedded": "^1.85.0"
  },
  "//": "Learn more: https://sli.dev/guide/write-theme.html",
  "slidev": {
    "colorSchema": "both",
    "defaults": {
      "fonts": {
        "sans": "Nunito Sans",
        "mono": "Fira Code"
      }
    }
  }
}